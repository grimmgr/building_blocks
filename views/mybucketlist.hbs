<div class="wrapper">
    {{> header }}
    <div class="container">
        <div class="title-container">
            <h2>{{bucketListItems.0.username}}'s Bucket List</h2>
            <div>
                <div class="list-container">
                    <ul class="user-list">
                        {{#each bucketListItems}}
                        {{>listItem}}
                        {{/each}}
                    </ul>
                </div>
            </div>
        </div>
    </div>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="/js/header.js"></script>
    <script src="/js/my-bucketlist.js"></script>
